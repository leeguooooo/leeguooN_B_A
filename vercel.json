{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/cron/update-cache.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron/update-cache",
      "schedule": "*/30 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}